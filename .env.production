# .env.production
# Production environment variables
# These are embedded at build time by Vite

# API Configuration - IMPORTANT: Use VITE_API_BASE_URL consistently
VITE_API_BASE_URL=https://api.mieszkaniownik.wsparcie.dev

# Google Maps API Configuration
VITE_GOOGLE_MAPS_API_KEY=

# Application Configuration
VITE_APP_TITLE=Mieszkaniownik
VITE_APP_DESCRIPTION=Your key to student housing
VITE_ENABLE_DEBUG=false
VITE_LOG_LEVEL=error

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# Google Analytics (if used)
# VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Sentry (if used for error tracking)
# VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx
# VITE_SENTRY_ENVIRONMENT=production

# OAuth Configuration (if applicable)
# VITE_GOOGLE_CLIENT_ID=your-google-client-id
# VITE_DISCORD_CLIENT_ID=your-discord-client-id
