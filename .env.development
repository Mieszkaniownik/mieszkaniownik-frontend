# .env.development
# Development environment variables
# These are embedded at build time by Vite

# API Configuration
VITE_API_URL=https://api.mieszkaniownik.wsparcie.dev

# Application Configuration
VITE_APP_TITLE=Mieszkaniownik (Dev)
VITE_APP_DESCRIPTION=Your key to student housing - Development
VITE_ENABLE_DEBUG=true
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# Google Analytics (disabled in dev)
# VITE_GA_MEASUREMENT_ID=

# Sentry (if used for error tracking)
# VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx
# VITE_SENTRY_ENVIRONMENT=development

# OAuth Configuration (if applicable)
# VITE_GOOGLE_CLIENT_ID=your-google-client-id-dev
# VITE_DISCORD_CLIENT_ID=your-discord-client-id-dev
